

local lib = loadstring(game:HttpGet"https://raw.githubusercontent.com/gxdev-rblx/Gx-lololoooollll/refs/heads/main/Vape.txt")()

local win = lib:Window("Vape Universal",Color3.fromRGB(255, 255, 255), Enum.KeyCode.R)

