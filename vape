tab3:Button("Fulbright", function()
    if not _G.FullBrightRunning then
        _G.FullBrightRunning = true

        local Lighting = game:GetService("Lighting")

        local function removeEffects()
            for _, v in pairs(Lighting:GetChildren()) do
                if v:IsA("ColorCorrectionEffect") or v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("Atmosphere") then
                    v:Destroy()
                end
            end
        end

        local function applySettings()
            Lighting.Brightness = 2
            Lighting.ClockTime = 14
            Lighting.FogEnd = 100000
            Lighting.GlobalShadows = false
            Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
            removeEffects()
        end

        applySettings()

        task.spawn(function()
            while _G.FullBrightRunning do
                applySettings()
                task.wait(1)
            end
        end)
    end
end)

tab3:Button("No fog", function()
    if not _G.NoFog then
        _G.NoFog = true
        local Lighting = game:GetService("Lighting")

        task.spawn(function()
            while _G.NoFog do
                Lighting.FogEnd = 100000
                for _, v in pairs(Lighting:GetDescendants()) do
                    if v:IsA("Atmosphere") then
                        v:Destroy()
                    end
                end
                task.wait(1)
            end
        end)
    end
end)

